{
  "extends": "airbnb/base",
  "env": {
    "node": true
  },
  "globals": {
    "_": true,
    "Game": true,
    "PathFinder": true,
    "ERR_NOT_IN_RANGE": true,
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": [1, { "props": false } ],
    "import/prefer-default-export": 0,
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, { "ignoreComments": true }]
  }
}